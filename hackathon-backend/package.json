{
  "name": "hackathon-backend",
  "version": "1.0.0",
  "description": "Backend API built with Express, Sequelize ORM, and MySQL for hackathon project.",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "nodemon src/server.js",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "npm run lint && npm run format",
    "seed": "node src/seeders/seedResources.js"
  },
  "keywords": [
    "express",
    "sequelize",
    "mysql",
    "backend",
    "api"
  ],
  "author": "Kristine Nyaga",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3"
  }
}
